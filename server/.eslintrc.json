{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "import/newline-after-import": "off",
    "import/no-extraneous-dependencies": "off",
    "global-require": "off",
    "no-console": "off"
  }
}
